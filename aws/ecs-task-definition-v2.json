{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "JenkinsApp-TaskDefinition-v2",
    "containerDefinitions": [
        {
            "name": "jenkinsapp-v2",
            "image": "436451757152.dkr.ecr.ap-southeast-2.amazonaws.com/myjenkinsapp:1.0.61",
            "portMappings": [
                {
                    "name": "ngnix-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "0.5 GB",
    "cpu": ".25 vCPU",
    "executionRoleArn": "arn:aws:iam::436451757152:role/ecsTaskExecutionRole"
}
